package com.test;

import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class Test1 {
	public static void main(String[] args) {
		String s = "562sbhas";
		Pattern p = Pattern.compile("\\d");
		Matcher m = p.matcher(s);
		int i = Integer.parseInt(m.group());
		System.out.println(i);
	}
}