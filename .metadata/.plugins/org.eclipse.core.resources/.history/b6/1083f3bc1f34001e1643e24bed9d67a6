package programs;

public class Pattern_3 {
	public static void main(String[] args) {
		int rows = 6;
		for (int i = 1; i < rows; i++) {
			for (int j = 1; j < i; j++) {
				System.out.print(" ");
			}
			for (int x = 1; x < rows - i; x++) {
				System.out.print(x + " ");
			}
			System.out.println();
		}
	}
}
