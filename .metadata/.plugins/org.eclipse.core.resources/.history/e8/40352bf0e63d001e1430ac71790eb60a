package com.test;

import java.util.Scanner;

public class Test1 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		String s = scan.nextLine();
		String rev = "";
		char ch;
		scan.close();
		for (int i = 0; i < s.length(); i++) {
			ch = s.charAt(i);
			rev = ch + rev;
		}
		System.out.println(rev);
	}
}