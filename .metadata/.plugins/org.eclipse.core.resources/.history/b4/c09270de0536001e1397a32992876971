package com.test;

import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class Test2 {
	public static void main(String[] args) {
		String s = "Hell0 007";
		Pattern p = Pattern.compile("\\d");
		String m = p.matcher(s).group();
		int x = Integer.parseInt(m);
		System.out.println(x);

	}
}
